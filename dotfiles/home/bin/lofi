#!/bin/bash

url='https://www.youtube.com/watch?v=5qap5aO4i9A'

case "$1" in
girl)
	url='https://www.youtube.com/watch?v=jfKfPfyJRdk'
	;;
racoon)
	url='https://www.youtube.com/watch?v=7NOSDKb0HlU'
	;;
nancy)
	url='https://www.youtube.com/watch?v=Cq0e1eDp_9k'
	;;
feen)
	url='https://www.twitch.tv/thedragonfeeney'
	;;
train)
	url='https://www.youtube.com/watch?v=-imG6CNF9kI'
	;;
cat)
	url='https://www.youtube.com/watch?v=c4yLOd_-X58'
	;;
puppy)
	url='https://www.youtube.com/watch?v=XLwYWCzlfIw'
	;;
esac


